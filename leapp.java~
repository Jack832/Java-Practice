import java.util.*;
import java.io.*;
class leapp
{
public static void main(String arg[])
	{
	int str;
    String year;
    //part1: checking for year digits
	   Scanner sc = new Scanner(System.in);
	   System.out.println("enter the year");
	   str=sc.nextInt();
	   year=new Integer(str).toString();
       int len=year.length();
		System.out.println("this is leap year"+len);
}}
